package factions;

import java.sql.ResultSet;

import main.Plugin;

import org.bukkit.Bukkit;
import org.bukkit.event.Event;
import org.bukkit.event.EventHandler;
import org.bukkit.event.Listener;
import org.bukkit.event.block.BlockEvent;
import org.bukkit.event.player.PlayerEvent;

public class Spawn extends Faction implements Listener {

	public Spawn(ResultSet data, Plugin plugin) {
		super(data);
		Bukkit.getPluginManager().registerEvents(this, plugin);
	}
	
	@EventHandler
	private void blockEvent(PlayerEvent event) {
		if (event.getPlayer().hasPermission("admin")) {
			event.se
		}
		try {
			event.getClass().getMethod("setCancelled", Boolean.class).invoke(true);
		} 
		catch (Exception e) {
			
		}
	}
}
