package main;

import java.util.logging.Logger;

import org.bukkit.plugin.java.JavaPlugin;

public class Plugin extends JavaPlugin {
	
	// Anything in onEnable is executed when the plugin is loaded/reloaded
	@Override
	public void onEnable() {
		Logger.getLogger("Minecraft").info("QCraft Plugin Loaded");
		new PlayerData(this);
	}

	// Anything in onDisable is executed when the plugin is unloaded
	@Override
	public void onDisable() {
		PlayerData.quit();
	}
}
